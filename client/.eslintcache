ading: false\r\n            }\r\n        case GET_PROJECT_DETAILS:\r\n            console.log('reducer payload id', payload._id)\r\n            console.log(payload)\r\n            console.log('reducer state.activeProject ', state.activeProject)\r\n            return {\r\n                ...state,\r\n                activeProject: [payload],\r\n                loading: false\r\n            }\r\n        case CREATE_PROJECT:\r\n            return {\r\n                ...state,\r\n                projects: [payload, ...state.projects]\r\n            }\r\n\r\n        case PROJECTS_LOADING:\r\n            return {\r\n                ...state,\r\n                loading:true\r\n            }\r\n        default:\r\n            return state;\r\n    }\r\n}","D:\\code\\my-code-journal\\client\\src\\components\\ProjectList.js",["76","77","78"],"import React, { useEffect } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport { Image, Item } from 'semantic-ui-react';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport { getProjects } from '../actions/projectActions';\r\n\r\nexport default () => {\r\n    const projects = useSelector(state => state.project.projects);\r\n    const test = useSelector(state => console.log('state', state))\r\n    const dispatch = useDispatch();\r\n    \r\n    console.log(projects)\r\n    useEffect(() => {\r\n        dispatch(getProjects());\r\n    }, [dispatch])\r\n\r\n    \r\n    return (\r\n\r\n        <Item.Group divided>\r\n            {projects.map(project => (\r\n            <Item key={project._id}>\r\n                <Item.Image size=\"small\" src={project.thumbnail} />\r\n                <Item.Content>\r\n                    <Item.Header><Link to={`/project/${project._id}`}>{project.title}</Link></Item.Header>\r\n                    <Item.Meta>{project.status}</Item.Meta>\r\n                    <Item.Description>{project.description}</Item.Description>\r\n                </Item.Content>\r\n            </Item>\r\n